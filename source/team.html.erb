<div class="container">
  <div class="about-content text-center stuff">
    <div class="row">
      <h1>This template is built with <i class="fa fa-heart"></i> by</h1>
      <ul class="list-inline text-center">
        <% ["aamazan", "Thorgold"].each do |gh_username| %>
          <li>
            <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{gh_username}", class: "img-circle avatar" %>
            <h3><i class="fa fa-github avatar-link"></i><%= link_to "#{gh_username}", "https://github.com/#{gh_username}", class: "avatar-link" %></h3>
          </li>
        <% end %>
      </ul>
    </div>
  </div>
</div>
